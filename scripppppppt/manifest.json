{
    "manifest_version": 3,
    "name": "FALG Extention",
    "version": "1.0",
    "permissions": [
        "storage",
        "scripting"
    ],
    "options_page": "options/options.html",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "modules/kururing.js",
                "modules/randomSearch.js",
                "modules/rickLink.js",
                "modules/rickImage.js",
                "content.js"
            ]
        }
    ],
    "icons": {
        "128": "icons/icon.png"
    },
    "action": {
        "default_icon": {
            "16": "icons/icon.png",
            "32": "icons/icon.png",
            "48": "icons/icon.png",
            "128": "icons/icon.png"
        }
    }
}